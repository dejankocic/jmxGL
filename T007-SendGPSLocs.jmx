            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import org.apache.commons.io.FileUtils; // necessary import
import org.apache.jmeter.services.FileServer;

int lines = 0;
File myDrivers = new File(FileServer.getFileServer().getBaseDir()+&quot;/T007_drivers.data&quot;);
try {
	lines = FileUtils.readLines(myDrivers).size();
} catch (Throwable ex) {
    log.error(&quot;Failed to find # of drivers&quot;, ex);
}
vars.put(&quot;lines&quot;, String.valueOf(lines)); // store the count into &quot;lines&quot; variable
</stringProp>
